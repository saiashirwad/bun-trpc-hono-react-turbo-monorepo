{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"dist/**"
			]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"clean": {
			"cache": false
		},
		"start": {
			"dependsOn": [
				"build"
			],
			"cache": false,
			"env": [
				"NODE_ENV"
			]
		}
	}
}